    "dev": "cross-env NODE_ENV=development webpack-dev-server --config scripts/config/webpack.config.dev.js --hot --inline --host 0.0.0.0 --disable-host-check",
    "lint": "echo tslint static/**/*.tsx",
    "serve": "http-server public / -g -o"
  },
